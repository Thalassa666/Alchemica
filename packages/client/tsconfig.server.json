{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ESNext"]  // Уберите "DOM" если оно есть
  },
  "include": ["server/**/*.ts"],
  "exclude": ["node_modules"]
}
